<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FullScreenProgressBar">
        <attr name="customLoader" format="reference" />
    </declare-styleable>

    <declare-styleable name="TitledTextView">
        <attr name="title" format="string" />
        <attr name="text" format="string" />
        <attr name="titlePosition" format="integer">
            <enum name="start" value="0" />
            <enum name="center" value="1" />
            <enum name="end" value="2" />
        </attr>
        <attr name="textPosition" format="integer">
            <enum name="start" value="0" />
            <enum name="center" value="1" />
            <enum name="end" value="2" />
        </attr>
        <attr name="textAppearance" format="reference" />
        <attr name="titleAppearance" format="reference" />
        <attr name="verticalOffset" format="dimension" />
    </declare-styleable>

</resources>